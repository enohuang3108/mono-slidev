{
  "name": "mono-slidev",
  "version": "1.0.0",
  "description": "A monorepo for multiple Slidev presentations with GitHub Pages deployment",
  "type": "module",
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "build:all": "pnpm -r run build",
